{
  "opts": {
    "template": "node_modules/better-docs",
    "destination" : "./dist/jsdocs/"
  },
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "plugins": [
      "node_modules/better-docs/category",
      "plugins/markdown"
  ],
  "source": {
    "include" : ["src/", "src/jmespath/"],
    "includePattern": "\\.js$"
  }
}
